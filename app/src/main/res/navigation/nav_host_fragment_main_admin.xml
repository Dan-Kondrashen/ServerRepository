<?xml version="1.0" encoding="utf-8"?>
<!--suppress NavigationFile -->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_host_fragment_content_main"
    app:startDestination="@id/mainFragmentView">
    <fragment
        android:id="@+id/userChoseFragment"
        android:name="ru.kondrashen.diplomappv20.presentation.fragments.ChosenUserFragment"
        android:label="@string/user_chose"
        tools:layout="@layout/chosen_user_fragment">
        <argument
            android:name="userId"
            app:argType="integer" />
        <argument
            android:name="adminUserId"
            app:argType="integer" />
        <action
            android:id="@+id/action_userChoseFragment_to_mainFragmentView"
            app:destination="@id/mainFragmentView" />
    </fragment>
    <fragment
        android:id="@+id/chatChoseFragment"
        android:name="ru.kondrashen.diplomappv20.presentation.fragments.ChatChoseFragment"
        android:label="ChatChose"
        tools:layout="@layout/chose_chat_fragment">
        <argument
            android:name="userId"
            app:argType="integer" />

        <action
            android:id="@+id/action_chatFragment_to_mainFragmentEmployee"
            app:destination="@id/mainFragmentView" />
        <action
            android:id="@+id/action_chatChoseFragment_to_chatFragment"
            app:destination="@id/chatFragment" />

        <action
            android:id="@+id/action_chatChoseFragment_to_mainFragmentEmployee"
            app:destination="@id/mainFragmentView" />

        <action
            android:id="@+id/action_chatChoseFragment_to_preferenceFragment"
            app:destination="@id/preferenceFragment" />


    </fragment>

    <fragment
        android:id="@+id/editDocumentFragment"
        android:name="ru.kondrashen.diplomappv20.presentation.fragments.EditDocumentFragment"
        android:label="@string/edit_document"
        tools:layout="@layout/create_document_employer_fragment">
        <argument
            android:name="userId"
            app:argType="integer" />
        <action
            android:id="@+id/action_editDocumentFragment_to_personalSpaceFragment"
            app:destination="@id/personalSpaceFragment" />

    </fragment>
    <fragment
        android:id="@+id/mainFragmentView"
        android:name="ru.kondrashen.diplomappv20.presentation.fragments.MainAdminFragment"
        android:label="EmployerMain"
        tools:layout="@layout/admin_main_page">
        <argument android:name="userId"
            app:argType="integer"/>
        <argument android:name="userType"
            app:argType="string"/>
        <action
            android:id="@+id/action_mainFragmentEmployee_to_chatChoseFragment"
            app:destination="@id/chatChoseFragment" />
        <action
            android:id="@+id/action_mainFragment_to_preferenceFragment"
            app:destination="@id/preferenceFragment" />
        <action
            android:id="@+id/action_mainFragmentView_to_userChoseFragment"
            app:destination="@id/userChoseFragment" />
    </fragment>
    <fragment
        android:id="@+id/preferenceFragment"
        android:name="ru.kondrashen.diplomappv20.presentation.fragments.AppPreferenceFragment"
        android:label="@string/action_settings"
        tools:layout="@layout/preference_fragment">

    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="ru.kondrashen.diplomappv20.presentation.fragments.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/document_chat_fragment">
        <argument
            android:name="userId"
            app:argType="integer" />
        <argument
            android:name="respId"
            app:argType="integer" />
        <action
            android:id="@+id/action_chatFragment_to_mainFragmentEmployee"
            app:destination="@id/mainFragmentView" />


    </fragment>

</navigation>